<%= will_paginate %>
<table class="table table-bordered table-hover">
  <tbody>
    <!--<td>
      <strong>Specified Burial</strong>
    </td>-->
    <td>
      <strong>Cemetery</strong>
    </td>
    <td>
      <strong>County</strong>
    </td>
    <td>
      <strong>Number of Related Burials</strong>
    </td>
    <% @monuments.each do |m| %>
    <tr class="row-clickable" href=<%= monument_path(:id => m.id_mon_level) %>>
      <td>
	<%= m.cemetery.cem_name %>
      </td>
      <td>
	<%= m.county.name %>
      </td>
      <td>
	<%= m.num_indiv %>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>
<%= will_paginate %>
